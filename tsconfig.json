{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": ["dom", "esnext"],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "baseUrl": "./",
    "paths": {
      "components/*": ["./src/components/*"],
      "store/*": ["./src/store/*"],
      "style/*": ["./src/style/*"]
    }
  }
}
